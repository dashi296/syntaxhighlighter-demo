{
  "name": "syntaxhighlighter-demo",
  "homepage": "https://dashi296.github.io/syntaxhighlighter-demo/",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.6.0",
    "@material-ui/icons": "^4.5.1",
    "@types/jest": "24.0.22",
    "@types/node": "12.12.6",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.3",
    "@types/react-syntax-highlighter": "^11.0.1",
